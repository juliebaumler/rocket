#: main.py:21 main.py:40
msgid "GIVE A LETTER: "
msgstr "mayka palach t'səm-bit: "

#: main.py:24
msgid "BYE!"
msgstr "aɬqi!"

#: main.py:33
msgid "GUESSED LETTER: "
msgstr "t'ɬunas-t'səm-bit: "

#: main.py:39
msgid "NOT A LETTER. TRY AGAIN.\n"
msgstr "wik t'səm-bit ukuk. munk-kakwa wəx\u0323t.\n"

#: main.py:51
msgid "MISSED!"
msgstr "t'sipi!"

msgid "USED ALREADY. TRY AGAIN."
#msgstr "(You used {guess} already. Try again.)"
msgstr "anqati nayka munk t'səm-bit {guess}. munk-t'ɬunas wəx\u0323t."

msgid "GUESSES LEFT: "
#msgstr "Remaining Guesses: "
msgstr "kʰa nayka munk-t'ɬunas: "

msgid "THE WORD WAS: {chosen_word}\n"
#msgstr "The word was: {chosen_word}\n"
msgstr "uk tənəs-wawa {chosen_word} ukuk.\n"

#: main.py:130
msgid "GREAT! YOU WIN!"
msgstr "dret ɬush! mayka tulu!"

#: main.py:133
msgid "DIDN'T WIN"
msgstr "wik mayka tulu. :( "

